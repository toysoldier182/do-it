<div class="container">
  <div class="row">
    <div class="col-6">
      <%= render 'new' %>
    </div>
    <div class="col-6">
      <h1>My todos</h1>
      <p>These are all my todos: </p>

      <ul>
        <% @todos.each do |todo| %>
          <p><%= todo.name %></p>
        <% end %>
      </ul>
    </div>
  </div>
</div>
